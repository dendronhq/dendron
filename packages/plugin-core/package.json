{
  "name": "@dendronhq/plugin-core",
  "private": true,
  "displayName": "dendron",
  "description": "Dendron is a hierarchal note taking tool that grows as you do. ",
  "publisher": "dendron",
  "version": "0.3.16",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/dendronhq/dendron.git",
    "directory": "packages/plugin-core"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "second brain",
    "markdown",
    "roam",
    "notes",
    "schema"
  ],
  "preview": true,
  "extensionDependencies": [],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Dendron",
      "properties": {
        "dendron.rootDir": {
          "type": "string",
          "default": false,
          "description": "Where your dendron workspace is located"
        },
        "dendron.skipPrompt": {
          "type": "boolean",
          "default": false,
          "description": "Whether dendron prompts for confirmation before certain actions"
        }
      }
    },
    "commands": [
      {
        "command": "dendron.lookup",
        "title": "Dendron: Lookup"
      },
      {
        "command": "dendron.createJournalNote",
        "title": "Dendron: New Journal Note"
      },
      {
        "command": "dendron.createScratchNote",
        "title": "Dendron: New Scratch Note"
      },
      {
        "command": "dendron.initWS",
        "title": "Dendron: Initialize Workspace"
      },
      {
        "command": "dendron.changeWS",
        "title": "Dendron: Change Workspace"
      },
      {
        "command": "dendron.deleteNode",
        "title": "Dendron: Delete Node"
      },
      {
        "command": "dendron.dev.resetConfig",
        "title": "Dev:Dendron: ResetConfig"
      }
    ],
    "keybindings": [
      {
        "command": "dendron.lookup",
        "mac": "cmd+l"
      },
      {
        "command": "dendron.deleteNode",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d"
      }
    ]
  },
  "icon": "assets/images/logo.png",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "lint": "eslint src --ext ts",
    "watch": "npm run compile -- --watch",
    "build": "npm run compile",
    "compile": "tsc -p tsconfig.build.json",
    "pretest": "echo stub",
    "test": "node ./out/test/runTest.js",
    "format": "prettier --write src '*.json' '*.js'"
  },
  "devDependencies": {
    "@types/execa": "^2.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.155",
    "@types/mocha": "^7.0.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "prettier": "2.0.5",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@dendronhq/common-all": "^0.3.16",
    "@dendronhq/common-server": "^0.3.16",
    "@dendronhq/engine-server": "^0.3.16",
    "execa": "^4.0.2",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0"
  }
}

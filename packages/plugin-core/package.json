        "keybindings": [
            {
                "command": "dendron.gotoNote",
                "key": "ctrl+enter",
                "when": "editorFocus"
            },
            {
                "command": "dendron.createDailyJournalNote",
                "key": "ctrl+shift+i",
                "mac": "cmd+shift+i"
            },
            {
                "command": "dendron.copyNoteLink",
                "key": "ctrl+shift+c",
                "mac": "cmd+shift+c",
                "when": "editorFocus"
            },
            {
                "command": "dendron.copyNoteRef",
                "key": "ctrl+shift+r",
                "mac": "cmd+shift+r",
                "when": "editorFocus"
            },
            {
                "command": "dendron.deleteNode",
                "key": "ctrl+shift+d",
                "mac": "cmd+shift+d"
            },
            {
                "command": "dendron.lookup",
                "mac": "cmd+L",
                "key": "ctrl+l"
            },
            {
                "command": "dendron.lookup",
                "key": "ctrl+shift+j",
                "mac": "cmd+shift+j",
                "args": {
                    "noteType": "journal"
                }
            },
            {
                "command": "dendron.lookup",
                "key": "ctrl+shift+s",
                "mac": "cmd+shift+s",
                "args": {
                    "noteType": "scratch",
                    "selectionType": "selection2link"
                }
            },
            {
                "command": "dendron.lookupSchema",
                "mac": "cmd+shift+L",
                "key": "ctrl+shift+l"
            },
            {
                "command": "dendron.goUpHierarchy",
                "mac": "cmd+shift+up",
                "key": "ctrl+shift+up",
                "when": "editorFocus"
            },
            {
                "command": "dendron.goNextHierarchy",
                "key": "ctrl+shift+]",
                "when": "editorFocus"
            },
            {
                "command": "dendron.goPrevHierarchy",
                "key": "ctrl+shift+[",
                "when": "editorFocus"
            },
            {

{
  "name": "@dendronhq/dendron-next-server",
  "private": true,
  "version": "0.75.1",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/dendronhq/dendron.git",
    "directory": "packages/dendron-next-server"
  },
  "scripts": {
    "local:dev": "env LOCAL=true STAGE=dev next dev",
    "local:prod": "env LOCAL=true STAGE=prod next dev",
    "local:prod:aws": "env LOCAL=true STAGE=oneoff_aws_prod next dev",
    "setup": "yarn gen:theme && yarn build:js",
    "dev": "next dev",
    "prod": "env STAGE=prod next dev",
    "watch": "echo 0",
    "build": "yarn setup && yarn build:dev",
    "build:dev": "cross-env CDN_BUILD=1 STAGE=dev next build && next export",
    "build:prod": "cross-env CDN_BUILD=1 STAGE=prod next build && next export",
    "start": "next start",
    "checkout:dev": "amplify env checkout dev",
    "deploy:dev": "env STAGE=dev amplify publish",
    "deploy:prod": "env STAGE=prod amplify publish",
    "gen:theme": "gulp less",
    "build:js": "gulp js",
    "format": "echo nop",
    "clean": "yarn rimraf public/*.css public/js  out node_modules",
    "rimraf": "npx rimraf"
  },
  "nohoist": [
    "**/common-all",
    "**/common-all/**"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@chakra-ui/react": "^1.0.0",
    "@dendronhq/common-all": "^0.75.1",
    "@dendronhq/common-frontend": "^0.75.1",
    "@dendronhq/engine-server": "^0.75.1",
    "@emotion/react": "^11.1.1",
    "@emotion/styled": "^11.0.0",
    "@next/bundle-analyzer": "^11.1.2",
    "@reduxjs/toolkit": "^1.5.1",
    "@types/react": "^17.0.5",
    "antd": "^4.15.5",
    "autoprefixer": "^10.2.5",
    "aws-amplify": "^3.3.23",
    "babel-plugin-import": "^1.13.3",
    "classnames": "^2.3.1",
    "cytoscape": "^3.19.0",
    "cytoscape-euler": "^1.2.2",
    "formik": "^2.2.5",
    "formik-antd": "^2.0.1",
    "framer-motion": "^3.2.2-rc.1",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-debug": "^4.0.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^9.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^4.17.20",
    "next": "^11.1.2",
    "postcss": "^8.2.13",
    "react": "^17.0.2",
    "react-css-theme-switcher": "^0.3.0",
    "react-d3-tree": "^1.16.1",
    "react-dom": "^17.0.2",
    "react-icons": "^3.11.0",
    "react-redux": "^7.2.3",
    "sass": "^1.35.1",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "@types/cytoscape": "^3.14.15",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.11.2",
    "@types/styled-components": "^5.1.3",
    "@types/yup": "^0.29.7",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-debug": "^4.0.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^9.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2"
  }
}

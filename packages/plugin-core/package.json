{
  "name": "@dendronhq/plugin-core",
  "private": true,
  "displayName": "dendron",
  "description": "Dendron is a hierarchal note taking tool that grows as you do. ",
  "publisher": "dendron",
  "version": "0.77.0",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "installConfig": {
    "hoistingLimits": "none"
  },
  "keywords": [
    "second brain",
    "markdown",
    "roam",
    "notes",
    "schema"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension.js",
  "extensionPack": [
    "dendron.dendron-paste-image"
  ],
  "contributes": {
    "languages": [
      {
        "id": "markdown",
        "extensions": [
          ".md"
        ],
        "aliases": [
          "markdown"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "dendron.sample",
          "name": "Sample View",
          "contextualTitle": "Sample View",
          "type": "webview",
          "when": "dendron:devMode"
        },
        {
          "id": "dendron.calendar-view",
          "name": "Calendar View",
          "contextualTitle": "Calendar View",
          "type": "webview",
          "when": "dendron:pluginActive"
        },
        {
          "id": "dendron.treeView",
          "name": "Tree View",
          "contextualTitle": "Tree View",
          "when": "dendron:pluginActive && !dendron:webUIEnabled",
          "icon": "media/icons/dendron-vscode.svg"
        },
        {
          "id": "dendron.tree-view",
          "name": "Tree View",
          "contextualTitle": "Tree View",
          "type": "webview",
          "when": "dendron:pluginActive && dendron:webUIEnabled",
          "icon": "media/icons/dendron-vscode.svg"
        },
        {
          "id": "dendron.lookup-view",
          "name": "Lookup View",
          "contextualTitle": "Lookup View",
          "type": "webview",
          "when": "dendron:pluginActive && dendron:noteLookupActive"
        },
        {
          "id": "dendron.backlinks",
          "name": "Backlinks",
          "when": "dendron:pluginActive"
        }
      ]
    },
    "categories": [
      "Other"
    ],

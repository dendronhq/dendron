{
  "name": "@dendronhq/plugin-core",
  "private": true,
  "displayName": "dendron",
  "description": "Dendron is a hierarchal note taking tool that grows as you do. ",
  "publisher": "dendron",
  "version": "0.50.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "installConfig": {
    "hoistingLimits": "none"
  },
  "keywords": [
    "second brain",
    "markdown",
    "roam",
    "notes",
    "schema"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension.js",
  "extensionPack": [
    "dendron.dendron-markdown-preview-enhanced",
    "dendron.dendron-paste-image"
  ],
  "contributes": {
    "languages": [
      {
        "id": "markdown",
        "extensions": [
          ".md"
        ],
        "aliases": [
          "markdown"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "dendron.sample",
          "name": "Sample View",
          "when": "dendron:devMode",
          "type": "webview"
        },
        {
          "id": "dendron.calendar-view",
          "name": "Calendar View",
          "when": "dendron:webUIEnabled",
          "type": "webview"
        },
        {
          "id": "dendron.treeView",
          "name": "Tree View",
          "when": "dendron:pluginActive && !dendron:webUIEnabled"
        },
        {
          "id": "dendron.tree-view",
          "name": "Tree View V2",
          "when": "dendron:webUIEnabled",
          "type": "webview"
        },
        {
          "id": "dendron.backlinks",
          "name": "Backlinks",
          "when": "dendron:pluginActive"
        }
      ]
    },
    "categories": [
      "Other"
    ],
    "commands": [
      {
        "command": "dendron.browseNote",
        "title": "Dendron: Browse Note",
        "desc": "Browse note on github"
      },

{
  "name": "@dendronhq/dendron-viz",
  "version": "0.97.0",
  "description": "dendron-viz",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/dendronhq/dendron.git",
    "directory": "packages/dendron-viz"
  },
  "author": {
    "name": "Dendron INC",
    "email": "eng@dendron.so",
    "url": "https://www.dendron.so"
  },
  "keywords": [
    "common"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "prepublishOnly": "yarn build",
    "prebuild": "yarn clean",
    "build": "node_modules/.bin/esbuild --target=es2019 ./src/index.jsx --bundle --platform=node --outfile=index.js",
    "buildDendron": "node_modules/.bin/esbuild --target=es2019 ./src/dendron.ts --bundle --platform=node --outfile=dendron.js",
    "prepare": "husky install",
    "typecheck": "yarn run tsc --noEmit --allowJs",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "npm run test:jest --",
    "watch": "yarn compile --watch"
  },
  "dependencies": {
    "@actions/artifact": "^0.5.2",
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.0",
    "@dendronhq/common-all": "^0.92.1",
    "@dendronhq/engine-server": "^0.92.1",
    "d3": "^7.0.0",
    "esbuild": "^0.12.15",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "@types/jest": "^27.0.1",
    "@types/micromatch": "^4.0.2",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
